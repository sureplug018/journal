<%- include('partials/userDashboardHeader.ejs') %>
<!--************************************
					Inner Banner Start
			*************************************-->
<div class="sj-innerbanner">
  <div class="container">
    <div class="row">
      <div class="col-12 col-sm-12 col-md-12 col-lg-12">
        <div class="sj-innerbannercontent">
          <h1>Contact Support</h1>
          <ol class="sj-breadcrumb">
            <li><a href="/">Home</a></li>
            <li><a href="javascript:void(0);">Support</a></li>
          </ol>
        </div>
      </div>
    </div>
  </div>
</div>
<!--************************************
					Inner Banner End
			*************************************-->
<!--************************************
					Main Start
			*************************************-->
<main id="sj-main" class="sj-main sj-haslayout sj-sectionspace">
  <div class="container">
    <div class="row">
      <div id="sj-twocolumns" class="sj-twocolumns">
        <%- include('partials/dashboardNav.ejs') %>
        <div class="col-12 col-sm-12 col-md-12 col-lg-8 col-xl-9 float-right">
          <div id="sj-content" class="sj-content">
            <div class="sj-addarticleholdcontent">
              <div class="sj-dashboardboxtitle">
                <h2>Contact Support</h2>
              </div>
              <div class="sj-acsettingthold">
                <div style="padding: 30px 30px;">
                  <form class="sj-formtheme sj-formlogin support-form-faq" style="margin-bottom: 50px;">
                    <fieldset>
                      <div class="row">
                        <div class="col-12 col-lg-6 col-xl-6 col-md-6">
                          <div class="form-group">
                            <input type="text" class="form-control" id="fullName" placeholder="Full Name">
                          </div>
                        </div>
                        <div class="col-12 col-lg-6 col-xl-6 col-md-6">
                          <div class="form-group">
                            <input type="text" class="form-control" id="email" placeholder="Email">
                          </div>
                        </div>
                        <div class="col-12 col-lg-6 col-xl-6 col-md-6">
                          <div class="form-group">
                            <input type="text" class="form-control" id="phoneNumber" placeholder="Phone Number">
                          </div>
                        </div>
                        <div class="col-12 col-lg-6 col-xl-6 col-md-6">
                          <div class="form-group">
                            <input type="text" class="form-control" id="subject" placeholder="Subject">
                          </div>
                        </div>
                      </div>
                      <div class="form-group">
                        <textarea class="form-control" placeholder="Message" id="message"></textarea>
                      </div>
                      <div class="sj-btnarea">
                        <button class="sj-btn sj-btnactive support-faq-btn">Send Message</button>
                      </div>
                    </fieldset>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
<!--************************************
					Main End
			*************************************-->
<%- include('partials/userDashboardFooter.ejs') %>